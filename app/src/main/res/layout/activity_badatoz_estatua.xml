        android:tag="figure1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imagen6drag"
        app:srcCompat="@drawable/imagen1" />


    <RelativeLayout
        android:id="@+id/relative"
        android:layout_width="402dp"
        android:layout_height="255dp"
        android:layout_marginBottom="300dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="382dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:srcCompat="@drawable/fondopuzle" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/con"
            android:layout_width="match_parent"
            android:layout_height="251dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">


            <ImageView
                android:id="@+id/imagen3target"
                android:layout_width="124dp"
                android:layout_height="83dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="-1dp"
                android:tag="figure3"
                app:layout_constraintBottom_toTopOf="@+id/imagen6target"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imagen2target"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/imagen3" />

            <ImageView
                android:id="@+id/imagen7target"
                android:layout_width="124dp"
                android:layout_height="83dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="-1dp"
                android:tag="figure7"
                app:layout_constraintEnd_toStartOf="@+id/imagen8target"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imagen4target"
                app:srcCompat="@drawable/imagen7" />

            <ImageView
                android:id="@+id/imagen8target"
                android:layout_width="124dp"
                android:layout_height="83dp"
                android:layout_marginTop="-1dp"
                android:tag="figure8"
                app:layout_constraintEnd_toStartOf="@+id/imagen9target"
                app:layout_constraintStart_toEndOf="@+id/imagen7target"
                app:layout_constraintTop_toBottomOf="@+id/imagen5target"
                app:srcCompat="@drawable/imagen8" />

            <ImageView
                android:id="@+id/imagen9target"
                android:layout_width="124dp"
                android:layout_height="83dp"
                android:layout_marginTop="-1dp"
                android:layout_marginEnd="20dp"
                android:tag="figure9"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imagen8target"
                app:layout_constraintTop_toBottomOf="@+id/imagen6target"
                app:srcCompat="@drawable/imagen9" />

            <ImageView
                android:id="@+id/imagen6target"
                android:layout_width="124dp"
                android:layout_height="83dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="8dp"
                android:tag="figure6"
                app:layout_constraintBottom_toTopOf="@+id/imagen9target"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imagen5target"
                app:layout_constraintTop_toBottomOf="@+id/imagen3target"
                app:srcCompat="@drawable/imagen6" />

            <ImageView
                android:id="@+id/imagen5target"
                android:layout_width="124dp"
                android:layout_height="83dp"
                android:tag="figure5"
                app:layout_constraintBottom_toTopOf="@+id/imagen8target"
                app:layout_constraintEnd_toStartOf="@+id/imagen6target"
                app:layout_constraintStart_toEndOf="@+id/imagen4target"
                app:layout_constraintTop_toBottomOf="@+id/imagen2target"
                app:srcCompat="@drawable/imagen5" />

            <ImageView
                android:id="@+id/imagen1target"
                android:layout_width="124dp"
                android:layout_height="83dp"
                android:layout_marginStart="20dp"
                android:layout_marginBottom="-1dp"
                android:tag="figure1"
                app:layout_constraintBottom_toTopOf="@+id/imagen4target"
                app:layout_constraintEnd_toStartOf="@+id/imagen2target"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/imagen1" />

            <ImageView
                android:id="@+id/imagen2target"
                android:layout_width="124dp"
                android:layout_height="83dp"
                android:layout_marginBottom="-1dp"
                android:tag="figure2"
                app:layout_constraintBottom_toTopOf="@+id/imagen5target"
                app:layout_constraintEnd_toStartOf="@+id/imagen3target"
                app:layout_constraintStart_toEndOf="@+id/imagen1target"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/imagen2" />

            <ImageView
                android:id="@+id/imagen4target"
                android:layout_width="124dp"
                android:layout_height="83dp"
                android:layout_marginStart="20dp"
                android:tag="figure4"
                app:layout_constraintBottom_toTopOf="@+id/imagen7target"
                app:layout_constraintEnd_toStartOf="@+id/imagen5target"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imagen1target"
                app:srcCompat="@drawable/imagen4" />

        </androidx.constraintlayout.widget.ConstraintLayout>
