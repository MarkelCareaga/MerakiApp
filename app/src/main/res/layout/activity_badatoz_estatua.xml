        app:srcCompat="@drawable/imagen7" />

    <ImageView
        android:id="@+id/imagen7drag"
        android:layout_width="114dp"
        android:layout_height="81dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="120dp"
        android:tag="figure3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imagen4drag"
        app:layout_constraintVertical_bias="0.518"
        app:srcCompat="@drawable/imagen3" />

    <ImageView
        android:id="@+id/imagen8drag"
        android:layout_width="114dp"
        android:layout_height="81dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="120dp"
        android:tag="figure5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imagen5drag"
        app:layout_constraintVertical_bias="0.518"
        app:srcCompat="@drawable/imagen5" />

    <ImageView
        android:id="@+id/imagen9drag"
        android:layout_width="114dp"
        android:layout_height="81dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="4dp"
