<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".juegos.BadatozEstatuaActivity">


    <ImageView
        android:id="@+id/imagen1drag"
        android:layout_width="114dp"
        android:layout_height="81dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="9dp"
        android:tag="figure8"
        app:layout_constraintBottom_toTopOf="@+id/imagen4drag"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relative"
        app:srcCompat="@drawable/imagen8" />

    <ImageView
        android:id="@+id/imagen2drag"
        android:layout_width="114dp"
        android:layout_height="81dp"
        android:layout_marginTop="9dp"
        android:tag="figure2"
        app:layout_constraintBottom_toTopOf="@+id/imagen5drag"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
