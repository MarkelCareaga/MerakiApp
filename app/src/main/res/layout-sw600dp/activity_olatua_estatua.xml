            app:layout_constraintStart_toEndOf="@+id/btn62B"
            app:layout_constraintTop_toBottomOf="@+id/txtEuskera" />

        <ImageButton
            android:id="@+id/btn12B"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="430dp"
            android:layout_marginTop="36dp"
            app:layout_constraintStart_toEndOf="@+id/btn11B"
            app:layout_constraintTop_toBottomOf="@+id/txtObjetivoOlatua"
            app:srcCompat="@drawable/ic_baseline_boton_radio"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <ImageButton
            android:id="@+id/btn22B"
            android:layout_width="wrap_content"
