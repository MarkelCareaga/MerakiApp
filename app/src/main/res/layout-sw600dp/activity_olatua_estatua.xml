            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtObjetivoOlatua"
            app:layout_constraintVertical_bias="1.0">

